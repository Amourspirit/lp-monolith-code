{
  "name": "monolithcode2",
  "version": "2.0.14",
  "description": "minimalistic coder editor",
  "homepage": "https://haeri.github.io/MonolithCode2/",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "npx electron-packager . monolith_code --icon=./res/img/icon --ignore=docs && cd updater && npm install && npm run package && cd ../monolith_code-* && npx cross-var \"mkdir $npm_package_version\" && npx cross-var \"cpx ../updater/updater.* ./$npm_package_version\""
  },
  "keywords": [
    "code",
    "editor"
  ],
  "author": "Haeri",
  "license": "MIT",
  "devDependencies": {
    "cpx": "^1.5.0",
    "cross-var": "^1.1.0",
    "electron": "^11.2.3",
    "electron-packager": "^15.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1"
  },
  "dependencies": {
    "highlight.js": "^10.6.0",
    "marked": "^2.0.0",
    "mime-types": "^2.1.29",
    "request": "^2.88.2",
    "tree-kill": "^1.2.2"
  }
}
